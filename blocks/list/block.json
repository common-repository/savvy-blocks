{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "savvy-blocks/list",
  "version": "0.1.0",
  "title": "List",
  "category": "savvy-blocks",
  "description": "Savvy List Block.",
  "keywords": [ "Savvy", "List" ],
  "textdomain": "savvy-blocks",
  "editorScript": "file:./build/index.js",
  "editorStyle": "file:./build/index.css",
  "style": "file:./build/style-index.css",
  "attributes": {
    "isAdvanced": {
      "type": "boolean",
      "default": true
    },
    "blockType" : {
      "type" : "string",
      "default": "custom"
    },
    "bullet": {
      "type": "object"
    },
    "listType": {
      "type": "string",
      "default": "ul"
    },
    "listStyle": {
      "type": "object"
    },
    "icon": {
      "type": "object"
    },
    "top": {
      "type": "object"
    },
    "gap": {
      "type": "object"
    },
    "margin": {
      "type": "object"
    },
    "padding": {
      "type": "object"
    }
  },
  "providesContext": {
    "bullet": "bullet"
  },
  "supports": {
    "html": false,
    "anchor": true
  }
}
