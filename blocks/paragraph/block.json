{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "savvy-blocks/paragraph",
  "version": "0.1.0",
  "title": "Paragraph",
  "category": "savvy-blocks",
  "description": "Savvy Paragraph Block.",
  "keywords": [ "Savvy", "Block" ],
  "textdomain": "savvy-blocks",
  "editorScript": "file:./build/index.js",
  "editorStyle": "file:./build/index.css",
  "style": "file:./build/style-index.css",
  "attributes": {
    "text" : {
      "type" : "string",
      "default": ""
    },
    "blockType" : {
      "type" : "string",
      "default": "custom"
    },
    "effects": {
      "type": "object"
    },
    "margin": {
      "type": "object"
    },
    "padding": {
      "type": "object"
    },
    "textAlign": {
      "type": "object"
    },
    "textColor": {
      "type": "string"
    }
  },
  "supports": {
    "anchor": true
  }
}
