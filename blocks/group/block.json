{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "savvy-blocks/group",
  "version": "0.1.0",
  "title": "Group",
  "category": "savvy-blocks",
  "description": "Savvy Group Block.",
  "keywords": [ "Savvy", "Group" ],
  "textdomain": "savvy-blocks",
  "editorScript": "file:./build/index.js",
  "editorStyle": "file:./build/index.css",
  "style": "file:./build/style-index.css",
  "attributes": {
    "display": {
      "type": "object"
    },
    "gap": {
      "type": "object"
    },
    "margin": {
      "type": "object"
    },
    "padding": {
      "type": "object"
    },
    "background": {
      "type": "object"
    },
    "textColor": {
      "type": "string"
    },
    "tagName": {
      "type": "string",
      "default": "div"
    },
    "animation": {
      "type": "object"
    }
  },
  "supports": {
    "html": false,
    "anchor": true
  }
}
