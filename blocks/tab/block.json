{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "savvy-blocks/tab",
  "version": "0.1.0",
  "title": "Tab",
  "category": "savvy-blocks",
  "icon": "layout",
  "description": "Savvy Tab Block.",
  "keywords": [ "Savvy", "Tab" ],
  "textdomain": "savvy-blocks",
  "editorScript": "file:./build/index.js",
  "editorStyle": "file:./build/index.css",
  "style": "file:./build/style-index.css",
  "attributes": {
    "tabIndex":{
      "type": "number"
    },
    "tabLabel":{
      "type": "string",
      "default": ""
    },
    "isActive": {
      "type": "boolean",
      "default": true
    },
		"style": {
			"type": "string"
		},
    "tabPaneId":{
			"type": "string"
		}
  },
  "supports": {
    "remove": false,
    "lock": false,
    "toolbar": false,
    "html": false,
    "anchor": true
  }
}